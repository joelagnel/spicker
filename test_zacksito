        <script>
        dhtml_searchbox.stock_quote_submit = function( _searchbox )
        {
            // Extract protocol from URL, then separate pieces
            var _url_components = String(window.location).split(':',2);
            var _protocol       = _url_components[0];
            _url_components     = _url_components[1].split('/');

            // Update the TICKER value with the selected value
            _url_components[5] = _searchbox.attr('value');

            // Rebuild URL
            var _action_url = _protocol + ':' + _url_components.join('/');

            // Update the FORM ACTION
            $(_searchbox).parents('form').attr('action', _action_url);
        };
        </script>
    </aside>


